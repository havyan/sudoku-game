<table class="table game-results-table">
  <thead>
    <th>排名</th>
    <th>用户名</th>
    <th>局分</th>
    <th>积分变化</th>
    <th>现积分</th>
    <th>天才币</th>
  </thead>
  <tbody>
    {{#results}}
    <tr>
      <td>{{rank}}</td>
      <td>{{playerName}}</td>
      <td>{{score}}</td>
      <td>+{{gainPoints}}</td>
      <td>{{points}}</td>
      <td>{{money}}</td>
    </tr>
    {{/results}}
  </tbody>
</table>
